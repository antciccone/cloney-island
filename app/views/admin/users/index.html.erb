<h1>Users</h1>
<table class="table table-striped table-hover table-responsive">
  <th>User's Name</th>
  <th>User's Status</th>
  <th>Status change</th>
<% @users.each_with_index do |user, index| %>
<div id=<%= "#{index}" %> >
  <tr><td><%= link_to user.name, user_path(user) %></td>
    <td><%= user.status %></td>
  <td><%= link_to "Change Status", admin_edit_user_path(user) %></td></tr>
</div>
<% end %>
</table>