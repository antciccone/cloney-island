<%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :goal %>
  <%= f.text_field :goal %>
  <%= f.collection_select :category_id, @categories,:id,:name, include_blank: true %> 
  <%= f.submit "Create Project!" %>
<%end%>
